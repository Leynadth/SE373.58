<P>Employee List</P>
<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Department</th>
        <th>Start Date</th>
        <th>Job Title</th>
        <th>Salary</th>
    </tr>
    {{#each employees}}
    <tr>
        <td>{{this.firstName}}</td>
        <td>{{this.lastName}}</td>
        <td>{{this.department}}</td>
        <td>{{this.startDate}}</td>
        <td>{{this.jobTitle}}</td>
        <td>${{this.salary}}</td>
        <td>
            <a href="/edit-employee/{{this._id}}">Edit</a>
            <form action="/delte-employee/{{this._id}}" method="POST">
                <button type="submit">Delete</button>
            </form>
        </td>
    </tr>
    {{/each}}
</table>
<a href="/addEmployee">Add a Employee</a>
